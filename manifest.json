{
"update_url": "https://clients2.google.com/service/update2/crx",

    "name": "Google Tasks™ on new tab",
    "version": "1.2",
    "description": "A simple extension that allows you to open Google Tasks™ in a new tab.",
    "manifest_version": 2,
    "permissions":[
    	"tabs",
    	"https://tasks.google.com/*/"
    ],
    "background":{
    	"scripts":["background.js"],
    	"persistent":true
    },
    "browser_action":{

    },
    "icons":{
    	"128":"googletasks128.png"
    }
  }